// comando para inicializar o schema npx prisma init --datasource-provider SQLite
// comando para criar a tabela npx prisma migrate dev
// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String

}